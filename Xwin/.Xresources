*.foreground: #c8c8c8
*.background: #002b36

! do not scroll with output
URxvt*scrollTtyOutput:	false

! scroll in relation to buffer
URxvt*scrollWithBuffer:	true

! scrollback to the bottom with keypress
URxvt*scrollTtyKeypress:	true

! DejaVu Sana Mono Font
URxvt*font: xft:DejaVu Sans Mono:style=Book:pixelsize=15
URxvt*boldFont: xft:DejaVu Sans Mono:style=bold:pixelsize=15
URxvt*italicFont: xft:DejaVu Sans Mono:style=Oblique:pixelsize=15
URxvt*boldItalicFont: xft:DejaVu Sans Mono:style=BoldOblique:pixelsize=15
URxvt.letterSpace: 0


! Other stupid configs
URxvt*scrollBar:	false
URxvt*saveLines:	20000

! Perl PLugins
URxvt.perl-ext-common: default,clipboard
URxvt.iso14755: False
URxvt.clipboard.autocopy: true


! Cursor
URxvt.cursorBlink:          true
URxvt.cursorColor:          #aa0000
URxvt.cursorUnderline:      false

! Pointer
URxvt.pointerBlank:         true


! white dark/light
*color7: #eee8d5
*color15: #fdf6e3

! Black
URxvt*color0: #000000
URxvt*color8: #555753

! Red
URxvt*color1: #cc0000
URxvt*color9: #ef2929

! Green
URxvt*color2: #4e9a06
URxvt*color10: #8ae234

! Yellow
URxvt*color3: #c4a000
URxvt*color11: #fce94f

! Blue
URxvt*color4: #00AFFF
URxvt*color12: #729fcf

! Purple
URxvt*color5: #75507b
URxvt*color13: #ad7fa8

! Cyan
URxvt*color6: #0b939b
URxvt*color14: #00f5e9




